{
  "name": "flex-tool-bar",
  "main": "./lib/flex-tool-bar",
  "version": "2.2.7",
  "description": "Easily Customizable ToolBar for Atom",
  "repository": "https://github.com/cakecatz/flex-toolbar",
  "license": "MIT",
  "keywords": [
    "toolbar",
    "tool-bar"
  ],
  "engines": {
    "atom": ">=1.27.0 <2.0.0"
  },
  "dependencies": {
    "atom-package-deps": "^8.0.0",
    "change-case": "^4.1.2",
    "cson": "^7.20.0",
    "fs-plus": "^3.1.1",
    "glob-to-regexp": "0.4.1",
    "json5": "^2.2.1"
  },
  "package-deps": [
    {
      "name": "tool-bar"
    }
  ],
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "atomTestRunner": "./spec/runner",
  "scripts": {
    "test": "atom --test ./spec && npm run lint",
    "lint": "remark . & eslint ."
  },
  "devDependencies": {
    "@semantic-release/apm-config": "^9.0.1",
    "atom-jasmine3-test-runner": "^5.2.12",
    "eslint": "^8.13.0",
    "remark-cli": "^10.0.1",
    "remark-preset-lint-recommended": "^6.1.2",
    "semantic-release": "^19.0.2"
  }
}
